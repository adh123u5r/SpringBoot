package com.user.day1.database;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;

	@Entity
	public class Game {
		
		@Id
		private int Gameid;
		private String Gamename;
		private String Gamemodel;
		private String Gamecolour;
		private String Gamevfx;
		private String Gamesoftware;
		private String Gamedesign;
		private String Gamememoryspace;
		private String Gamehero;
		public int getGameid() {
			return Gameid;
		}
		public void setGameid(int gameid) {
			Gameid = gameid;
		}
		public String getGamename() {
			return Gamename;
		}
		public void setGamename(String gamename) {
			Gamename = gamename;
		}
		public String getGamemodel() {
			return Gamemodel;
		}
		public void setGamemodel(String gamemodel) {
			Gamemodel = gamemodel;
		}
		public String getGamecolour() {
			return Gamecolour;
		}
		public void setGamecolour(String gamecolour) {
			Gamecolour = gamecolour;
		}
		public String getGamevfx() {
			return Gamevfx;
		}
		public void setGamevfx(String gamevfx) {
			Gamevfx = gamevfx;
		}
		public String getGamesoftware() {
			return Gamesoftware;
		}
		public void setGamesoftware(String gamesoftware) {
			Gamesoftware = gamesoftware;
		}
		public String getGamedesign() {
			return Gamedesign;
		}
		public void setGamedesign(String gamedesign) {
			Gamedesign = gamedesign;
		}
		public String getGamememoryspace() {
			return Gamememoryspace;
		}
		public void setGamememoryspace(String gamememoryspace) {
			Gamememoryspace = gamememoryspace;
		}
		public String getGamehero() {
			return Gamehero;
		}
		public void setGamehero(String gamehero) {
			Gamehero = gamehero;
		}
		@Override
		public String toString() {
			return "Game [Gameid=" + Gameid + ", Gamename=" + Gamename + ", Gamemodel=" + Gamemodel + ", Gamecolour="
					+ Gamecolour + ", Gamevfx=" + Gamevfx + ", Gamesoftware=" + Gamesoftware + ", Gamedesign="
					+ Gamedesign + ", Gamememoryspace=" + Gamememoryspace + ", Gamehero=" + Gamehero + "]";
		}
	}
		
		